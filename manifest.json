{
  "manifest_version": 2,
  "name": "Player Youtube",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/icon.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.music.youtube.com/*"],
      "js": ["body.js"]
    }
  ]

}

  
